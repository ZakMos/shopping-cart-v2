{
  "name": "shopping-cart",
  "version": "1.0.0",
  "private": true,
  "homepage": "http://zakproject01.epizy.com/",
  "description": "simple small shopping site example",
  "author": "Zakaria Moslim",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.2",
    "bootstrap": "^4.3.1",
    "font-awesome": "^4.7.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-scripts": "^3.4.0",
    "redux": "^4.0.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/sass/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar src/sass/ -o src/styles/ --watch --recursive",
    "build-js": "react-scripts build",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react-script": "^2.0.5"
  }
}
