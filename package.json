{
  "name": "shopping-cart",
  "version": "1.0.0",
  "private": true,
  "homepage": "http://zakproject01.epizy.com/",
  "description": "simple small shopping site example",
  "author": "Zakaria Moslim",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.2",
    "bootstrap": "^4.6.0",
    "font-awesome": "^4.7.0",
    "gh-pages": "^3.2.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^5.0.7",
    "react-scripts": "^3.4.4",
    "redux": "^4.1.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/sass/ -o src/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar src/sass/ -o src/styles/ --watch --recursive",
    "build-js": "react-scripts build",
    "deploy": "gh-pages -b master -d build",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all react-scripts build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "react-script": "^2.0.5"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZakMos/shopping-cart-v2.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ZakMos/shopping-cart-v2/issues"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
